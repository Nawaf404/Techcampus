
def main():
    dict()
    while True:
        dic = {'name':'python','age':'1991','source?':'open','why':'web,GUI,Scripting','high or low'
        :'high','whats code for display on Screen?':'print'}
        Words = ('name','age','source?','why','high or low','whats code for display on Screen?')
        print(Words, "\n", "choose","\n")
        User = input("Type word : ")
        Transfer = dic.get(User)
        print(Transfer)
        print("\n", "Thanks again ! ", "\n")
main()
