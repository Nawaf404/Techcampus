import turtle
jack = turtle.Turtle()
jack.color("yellow")
jack.width(5)
def draw_square(length):
    for n in range(4):
        jack.forward(length)
        jack.right(90)

def main():
    jack.penup()
    jack.left(90)
    jack.forward(50)
    jack.pendown()
    
draw_square(100)
main()
draw_square(100)
main()
draw_square(100)
main()
draw_square(100)
## That's so Easy !! 
